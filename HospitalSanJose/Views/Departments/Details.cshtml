@model HospitalSanJoseModel.Department

@{
    ViewData["Title"] = "Departamentos";
}



<div class="card p-3">
    <div class="d-flex justify-content-between">
        <h3>Información de Departamento</h3>
        <a class="align-self-end" role="button" onclick="window.history.back()">Regresar </a>
    </div>

    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DepartmentName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DepartmentName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Description)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Description)
        </dd>
    </dl>
<div>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model?.Id">Editar Departamento</a> 
</div>
</div>
